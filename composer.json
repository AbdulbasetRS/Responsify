{
    "name": "abdulbaset/responsify",
    "description": "A Laravel package for standardized API responses",
    "type": "library",
    "license": "MIT",
    "homepage": "https://www.linkedin.com/in/abdulbaset-r-sayed/",
    "authors": [
        {
            "name": "Abdulbaset Reda Sayed",
            "email": "AbdulbasetRedaSayedHF@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Abdulbaset\\Responsify\\": "src/"
        },
        "files": [
            "src/Helpers/ResponsifyHelper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abdulbaset\\Responsify\\Providers\\ResponsifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
